@model IEnumerable<HRM.DAL.Entity.AspNetUsersEntity>

@{
    ViewBag.Title = "UserList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-agrani">
            <div class="panel-heading">
                User List <a href="@Url.Action("Register", "Account")" style="text-decoration:none;" data-toggle="tooltip" title="Add New User" class="btn btn-default btn-sm"><i class="glyphicon glyphicon-edit"></i></a>
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body" id="mform">

               
                <table class="table table-responsive table-bordered table-hover">
                    <tr style="color: #fff; background-color: #007500;">
                        <th>
                            @Html.DisplayNameFor(model => model.UserName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.BranchCode)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.BranchName)
                        </th>



                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.UserName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.BranchCode)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.BranchName)
                            </td>


                        </tr>
                    }

                </table>
            </div>
        </div>

    </div>

</div>